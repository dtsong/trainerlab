{
  "name": "@trainerlab/shared-types",
  "version": "0.0.1",
  "private": true,
  "description": "Shared TypeScript types for TrainerLab",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "files": [
    "src"
  ],
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist node_modules"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^2.1.0",
    "typescript": "^5.3.0",
    "vitest": "^2.1.0"
  }
}
